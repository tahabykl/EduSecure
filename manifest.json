{
  "manifest_version": 3,
  "name": "EduSecure",
  "version": "1.0",
  "description":"Öğrenme Yönetim Sistemlerinde bilgi güvenliğinin sağlanması ve farkındalığın artırılması için bir tarayıcı eklentisi.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "host_permissions": ["http://*/", "https://*/"],
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "scripting"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["login.html"],
      "matches": ["<all_urls>"]
    }
  ]
}